@model IEnumerable<MusicStore.Models.Album>

@{
    ViewBag.Title = "Search";
}

<h2>Search</h2>

<table class="table tab-content table-bordered table-striped">
    <tr>
        <th>Artist</th>
        <th>Title</th>
        <th>Price</th>
    </tr>

  @foreach (var item in Model)
  {
      
        <tr>
            <td>@item.artist.Name</td>
            <td>@item.Title</td>
            <td>@item.Price</td>
        </tr>
    }

</table>  